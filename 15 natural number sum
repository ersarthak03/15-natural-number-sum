#include<stdio.h>
    int main(){
        int sum=0;
        //for(int i=1;i<=15;i++){
           //sum=i+sum;
        //}
         int i=1;
        while( i<=15){
            sum=sum+i;
            i++;
            }
            
        
        printf("sum of 15 natural number is %d", sum);
        return 0;
    
    }
